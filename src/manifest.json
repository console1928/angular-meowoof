{
  "manifest_version": 3,
  "name": "Meowoof",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html#/popup"
  },
  "description": "Extension for cats and dogs lovers",
  "icons": {
    "48": "assets/images/logo.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs"
  ]
}
